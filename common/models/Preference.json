{
  "name": "Preference",
  "plural": "Preferences",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "birthDate": {
      "type": "date"
    },
    "student": {
      "type": "boolean",
      "required": "false"
    },
    "gender": {
      "type": "string",
      "required": "false"
    },
    "matriculationNr": {
      "type": "number"
    },
    "course": {
      "type": "string"
    },
    "operatingSystem": {
      "type": {
        "android": "boolean",
        "ios": "boolean",
        "other": "boolean"
      },
      "required": false
    },
    "handedness": {
      "type": "string",
      "required": false,
      "description": "left, right, both"
    },
    "visualAid": {
      "type": {
        "glasses": "boolean",
        "contactLenses": "boolean",
        "none": "boolean"
      },
      "required": false
    },
    "language": {
      "type": {
        "english": "boolean",
        "german": "boolean"
      },
      "required": false
    },
    "height": {
      "type": "number",
      "required": "false",
      "description": "in cm"
    }
  },
  "validations": [],
  "relations": {
    "subuser": {
      "type": "belongsTo",
      "model": "Subuser",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "create"
    }
  ],
  "methods": {}
}
